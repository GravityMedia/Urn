{
    "name": "gravitymedia/urn",
    "description": "A PHP library for generating RFC 2141 compliant uniform resource names (URN).",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel SchrÃ¶der",
            "email": "daniel.schroeder@gravitymedia.de"
        }
    ],
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.4",
        "scrutinizer/ocular": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "GravityMedia\\Urn\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GravityMedia\\UrnTest\\": "tests/src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "archive": {
        "exclude": [
            "/.idea",
            "/.*",
            "/composer.*",
            "/phpunit.*",
            "/tests"
        ]
    }
}
